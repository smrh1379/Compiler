            // fall through
          case 135: break;
          case 53:
            { htmlHighlighter.stringsAndCharacters (string);
                string = "";
                  htmlHighlighter.specialCharacters(yytext());
                  return symbol("Special_CHAR", "\\\"" );
            }
            // fall through
          case 136: break;
          case 54:
            { htmlHighlighter.stringsAndCharacters (string);
                string = "";
