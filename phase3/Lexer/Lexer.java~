                  return symbol("Special_CHAR", "\\n" );
            }
            // fall through
          case 130: break;
          case 48:
            { htmlHighlighter.specialCharacters(yytext());
                  return symbol("Special_CHAR", "\\r" );
            }
            // fall through
          case 131: break;
          case 49:
            { htmlHighlighter.specialCharacters(yytext());
                  return symbol("Special_CHAR", "\\t" );
            }
            // fall through
          case 132: break;
          case 50:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("IF",yytext());
            }
            // fall through
          case 133: break;
          case 51:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("IN",yytext());
            }
            // fall through
          case 134: break;
          case 52:
            { htmlHighlighter.operatorsAndPunctuations(yytext());
                    return symbol("LOGICOR",yytext());
            }
            // fall through
          case 135: break;
          case 53:
            { htmlHighlighter.stringsAndCharacters (string);
                string = "";
                  htmlHighlighter.specialCharacters(yytext());
                  return symbol("Special_CHAR", "\\\"" );
            }
            // fall through
          case 136: break;
          case 54:
            { htmlHighlighter.stringsAndCharacters (string);
                string = "";
                  htmlHighlighter.specialCharacters(yytext());
                  return symbol("Special_CHAR", "\\'" );
            }
            // fall through
          case 137: break;
          case 55:
            { htmlHighlighter.stringsAndCharacters (string);
                string = "";
                  htmlHighlighter.specialCharacters(yytext());
                  return symbol("Special_CHAR", "\\\\" );
            }
            // fall through
          case 138: break;
          case 56:
            { htmlHighlighter.stringsAndCharacters (string);
                string = "";
                  htmlHighlighter.specialCharacters(yytext());
                  return symbol("Special_CHAR", "\\n" );
            }
            // fall through
          case 139: break;
          case 57:
            { htmlHighlighter.stringsAndCharacters (string);
                string = "";
                  htmlHighlighter.specialCharacters(yytext());
                  return symbol("Special_CHAR", "\\r" );
            }
            // fall through
          case 140: break;
          case 58:
            { htmlHighlighter.stringsAndCharacters (string);
                string = "";
                  htmlHighlighter.specialCharacters(yytext());
                  return symbol("Special_CHAR", "\\t" );
            }
            // fall through
          case 141: break;
          case 59:
            { htmlHighlighter.realNumbers(yytext());
                        return symbol("SCIFLOAT",yytext());
            }
            // fall through
          case 142: break;
          case 60:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("FOR",yytext());
            }
            // fall through
          case 143: break;
          case 61:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("INT",yytext());
            }
            // fall through
          case 144: break;
          case 62:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("LEN",yytext());
            }
            // fall through
          case 145: break;
          case 63:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("LET",yytext());
            }
            // fall through
          case 146: break;
          case 64:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("NEW",yytext());
            }
            // fall through
          case 147: break;
          case 65:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("BOOL",yytext());
            }
            // fall through
          case 148: break;
          case 66:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("ELSE",yytext());
            }
            // fall through
          case 149: break;
          case 67:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("FUNC",yytext());
            }
            // fall through
          case 150: break;
          case 68:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("LOOP",yytext());
            }
            // fall through
          case 151: break;
          case 69:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("REAL",yytext());
            }
            // fall through
          case 152: break;
          case 70:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("TRUE",yytext());
            }
            // fall through
          case 153: break;
          case 71:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("VOID",yytext());
            }
            // fall through
          case 154: break;
          case 72:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("BREAK",yytext());
            }
            // fall through
          case 155: break;
          case 73:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("CLASS",yytext());
            }
            // fall through
          case 156: break;
          case 74:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("FALSE",yytext());
            }
            // fall through
          case 157: break;
          case 75:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("PRINT",yytext());
            }
            // fall through
          case 158: break;
          case 76:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("RANGE",yytext());
            }
            // fall through
          case 159: break;
          case 77:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("WHILE",yytext());
            }
            // fall through
          case 160: break;
          case 78:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("RETURN",yytext());
            }
            // fall through
          case 161: break;
          case 79:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("STATIC",yytext());
            }
            // fall through
          case 162: break;
          case 80:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("STRING",yytext());
            }
            // fall through
          case 163: break;
          case 81:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("CONTINUE",yytext());
            }
            // fall through
          case 164: break;
          case 82:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("INPUTINT",yytext());
            }
            // fall through
          case 165: break;
          case 83:
            { htmlHighlighter.reservedKeyWords(yytext());
                  return symbol("INPUTSTR",yytext());
            }
            // fall through
          case 166: break;
          default:
            zzScanError(ZZ_NO_MATCH);
        }
      }
    }
  }


